<div class='content'>
  <h1><%= t('.header') %></h1>
  <p><%= t('.about_partnerships') %></p>
  <h3><%= t('.interested') %></h3>
  <div class='reach-out'>
      <%= mail_to PARTNERSHIPS[:email], t('.reach_out'), class: 'button mail' %>
  </div>

  <% @sponsors.each do |partner| %>
    <article class='partner'>
      <%= cl_image_tag partner.logo, width: 350, height: 300, crop: :fill %>
      <div class='info'>
        <h2><%= partner.name %></h2>
        <p><%= simple_format partner.description %></p>
      </div>
    </article>
  <% end %>
</div>
